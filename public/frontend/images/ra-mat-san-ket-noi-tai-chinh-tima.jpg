        function page_click(value) {
            var currentPage = parseInt($("#currentPage").val());
            var totalPage = parseInt($("#totalPage").val());
            var numberItem = $("#numberItem").val();
            //alert(numberItem);

            currentPage = currentPage + value;

            if (currentPage < 1 || currentPage > totalPage) return;

            $("#currentPage").val(currentPage);

            $('#lblCurrentPage').text(currentPage);
            var idCate = 1;
            $.ajax({
                type: "POST",
                url: "/News/PagingListNews",
                data: {idCate:idCate, CurrentPage: currentPage, Top: numberItem }
            }).done(function (data) {
                $('#listitemnews').html(data);
            });
        }
    </script>


